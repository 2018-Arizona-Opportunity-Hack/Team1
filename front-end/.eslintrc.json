{
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "plugin:react/recommended"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "import",
      "react"
  ],
  "settings": {
      "react": {
          "version" : "16.0.0"
      }
  }
}